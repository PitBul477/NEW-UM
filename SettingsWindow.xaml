<Window x:Class="NEW_UM.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NEW_UM"
        mc:Ignorable="d"
        Title="A" Height="450" Width="800">
    <Grid>
		<Button Click="Button_Click" Content="Интервал первого раунда" HorizontalAlignment="Left" Margin="100,18,0,0" VerticalAlignment="Top" Width="155"/>
		<TextBox x:Name="setting1" HorizontalAlignment="Left" Margin="23,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60"
         PreviewTextInput="TextBox_PreviewTextInput"/>
		<TextBox HorizontalAlignment="Left" Margin="23,64,0,0" TextWrapping="Wrap" Text="{Binding Setting2}" VerticalAlignment="Top" Width="60"/>
		<Button Click="Button_Click" Content="Button1" HorizontalAlignment="Left" Margin="100,64,0,0" VerticalAlignment="Top"/>
		<!--TextBox Text="{Binding Setting1}" />
		<TextBox Text="{Binding Setting2}" /-->
	</Grid>
</Window>
